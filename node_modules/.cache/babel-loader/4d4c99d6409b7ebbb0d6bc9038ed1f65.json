,MAAqC;AAA9B,IAAA,EAAE,EAAC,MAA2B;;aAAN,KAAA,CAAA,IAAA,GAAI,M;;AAAE,GAAjD,E,IAAA,E;;AAAA,G,iBAA2C,KAAA,CAAA,I;UAAd;MACzB,CAHN,C,EAKA,YAAA,CAAmF,QAAnF,EAAmF;AAA3E,aAAM,aAAqE;AAAtD,IAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,MAAL,KAAW;AAAiC,GAAnF,EAA0D,kBAA1D,EAA0E;AAAA;AAA1E,IAA0E,CAAA,UAAA,CAA1E,C,CARF,E;;AAAA,GAmBI,EARc,KAAA,CAAA,O,kBAAlB,YAAA,CAAwC,qBAAxC,EAAwC;AAAA,IAAA,GAAA,EAAA;AAAA,GAAxC,C,mBAEA,YAAA,CAKmB,0BALnB,EAKmB;UAAA;AAHd,IAAA,MAAM,EAAE,KAAA,CAAA,MAGM;AAFd,IAAA,MAAI,EAAE,QAAA,CAAA,UAEQ;AADd,IAAA,QAAM,EAAE,QAAA,CAAA;AACM,GALnB,E,IAAA,E;;AAAA,I,gCAAA,C,CAMI,CAtBN,C","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <app-alert :alert=\"alert\" @close=\"alert = null\"></app-alert>\r\n\r\n    <form class=\"card\" @submit.prevent=\"createPerson\">\r\n      <h2>Работа с базой данных</h2>\r\n\r\n      <div class=\"form-control\">\r\n        <label for=\"name\">Введите имя</label>\r\n        <input type=\"text\" id=\"name\" v-model.trim=\"name\">\r\n      </div>\r\n\r\n      <button class=\"btn primary\" :disabled=\"name.length === 0\">Создать человека</button>\r\n    </form>\r\n\r\n    <app-loader v-if=\"loading\"></app-loader>\r\n\r\n    <app-people-list\r\n        v-else\r\n        :people=\"people\"\r\n        @load=\"loadPeople\"\r\n        @remove=\"removePerson\"\r\n    ></app-people-list>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppPeopleList from './AppPeopleList'\r\nimport AppAlert from './AppAlert'\r\nimport AppLoader from './AppLoader'\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      people: [],\r\n      alert: null,\r\n      loading: false\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadPeople()\r\n  },\r\n  methods: {\r\n    async createPerson() {\r\n      const response = await fetch('https://vue-kurs-db-default-rtdb.firebaseio.com/people.json', {\r\n        method: 'POST',\r\n        headers: {\r\n          'Content-Type': 'application/json'\r\n        },\r\n        body: JSON.stringify({\r\n          firstName: this.name\r\n        })\r\n      })\r\n\r\n      const firebaseData = await response.json()\r\n\r\n      this.people.push({\r\n        firstName: this.name,\r\n        id: firebaseData.name\r\n      })\r\n      this.alert = {\r\n        type: 'primary',\r\n        title: 'Успешно!',\r\n        text: `Пользователь с именем \"${this.name}\" был добавлен`\r\n      }\r\n\r\n      this.name = ''\r\n    },\r\n    loadPeople() {\r\n      this.loading = true\r\n      setTimeout(async () => {\r\n        try {\r\n          const {data} = await axios.get('https://vue-kurs-db-default-rtdb.firebaseio.com/people.json')\r\n          if (!data) {\r\n            throw new Error('Список людей пуст')\r\n          }\r\n\r\n          this.people = Object.keys(data).map(key => {\r\n            return {\r\n              id: key,\r\n              ...data[key]\r\n            }\r\n          })\r\n          this.loading = false\r\n        } catch (e) {\r\n          this.alert = {\r\n            type: 'danger',\r\n            title: 'Ошибка!',\r\n            text: e.message\r\n          }\r\n          this.loading = false\r\n          console.log(e.message)\r\n        }\r\n      }, 1500)\r\n    },\r\n    async removePerson(id) {\r\n      try {\r\n        const name = this.people.find(person => person.id === id).firstName\r\n        await axios.delete(`https://vue-kurs-db-default-rtdb.firebaseio.com/people/${id}.json`)\r\n        this.people = this.people.filter(person => person.id !== id)\r\n        this.alert = {\r\n          type: 'primary',\r\n          title: 'Успешно!',\r\n          text: `Пользователь с именем \"${name}\" был удален`\r\n        }\r\n      } catch (e) {\r\n\r\n      }\r\n    }\r\n  },\r\n  components: {AppPeopleList, AppAlert, AppLoader}\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}